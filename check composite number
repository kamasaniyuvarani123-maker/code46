#include <stdio.h>
int main() {
    int n, i, count = 0;
 printf("Enter a number: ");
    scanf("%d", &n);
 if (n <= 1) {
        printf("Not a composite number");
        return 0;
    }
 for (i = 1; i <= n; i++) {
        if (n % i == 0) {
            count++;
        }
    }
 if (count > 2)
        printf("%d is a Composite number", n);
    else
        printf("%d is NOT a Composite number", n);
 return 0;
}
